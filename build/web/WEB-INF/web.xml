<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.Servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>com.Servlets.AdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>admin</servlet-name>
        <url-pattern>/adminservlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>com.Servlets.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addcity</servlet-name>
        <servlet-class>com.Servlets.AddCity</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addcity</servlet-name>
        <url-pattern>/addcity</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>removecityservlet</servlet-name>
        <servlet-class>com.Servlets.removeCityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>removecityservlet</servlet-name>
        <url-pattern>/removecityservlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>oddeven</servlet-name>
        <servlet-class>com.Servlets.OddEven</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>oddeven</servlet-name>
        <url-pattern>/oddeven</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>existedcity</servlet-name>
        <servlet-class>com.Servlets.ExistedCity</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>existedcity</servlet-name>
        <url-pattern>/existedcity</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>freecookie</servlet-name>
        <servlet-class>com.Servlets.FreeCookies</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>freecookie</servlet-name>
        <url-pattern>/freecookie</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
